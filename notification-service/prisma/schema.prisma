generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model notifications {
  id           Int       @id @default(autoincrement())
  title        String    @db.VarChar(40)
  body         String
  subscribers  Decimal[] @db.Decimal
  types        String[]
  date_created String
  date_read    String?
}

model users {
  id           Int     @id @default(autoincrement())
  userdata     String  @unique
  active_app   Boolean
  active_email Boolean
}
